
@{
    ViewBag.Title = "LosujDlaZaladowanegoCsv";
}

<h2>Załaduj plik CSV</h2>

<br/><br/>
@using (Html.BeginForm("LosujDlaZaladowanegoCsv", "Losowanie", FormMethod.Post, new { enctype = "multipart/form-data", @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="file" id="csvFile" name="csvFile" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Wczytaj plik teraz" class="btn btn-default" />
        </div>
    </div>
}


